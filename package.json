{
  "name": "wasm-benchmark",
  "version": "1.0.0",
  "description": "WebAssembly Performance Benchmark: Rust vs TinyGo Comparison",
  "main": "harness/web/bench.js",
  "type": "module",
  "scripts": {
    "postinstall": "echo 'Dependencies installed successfully'",
    "dev": "python3 -m http.server 3001 --directory harness/web",
    "serve": "python3 -m http.server 3001 --directory harness/web",
    "serve:port": "python3 -m http.server ${PORT:-3001} --directory harness/web",
    "build": "npm run build:config && scripts/build_all.sh",
    "build:config": "node scripts/build_config.js",
    "build:rust": "scripts/build_rust.sh", 
    "build:tinygo": "scripts/build_tinygo.sh",
    "bench": "node scripts/run_browser_bench.js",
    "bench:headed": "node scripts/run_browser_bench.js --headed",
    "bench:quick": "node scripts/run_browser_bench.js --timeout=60000",
    "analyze": "python3 analysis/statistics.py results/$(ls -t results/ | head -n1) && python3 analysis/plots.py results/$(ls -t results/ | head -n1)",
    "fingerprint": "scripts/fingerprint.sh",
    "all": "scripts/all_in_one.sh",
    "all:quick": "scripts/all_in_one.sh --quick",
    "clean": "rm -rf builds/* results/20* && find . -name '*.tmp' -delete",
    "test": "echo 'No tests defined yet'",
    "lint": "echo 'No linting configured yet'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alleninnz/wasm-benchmark.git"
  },
  "keywords": [
    "webassembly",
    "wasm", 
    "rust",
    "go",
    "tinygo",
    "benchmark",
    "performance",
    "comparison"
  ],
  "author": "Xingxing Wang",
  "license": "MIT",
  "dependencies": {
    "puppeteer": "24.19.0",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@types/node": "24.3.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "homepage": "https://github.com/alleninnz/wasm-benchmark#readme"
}